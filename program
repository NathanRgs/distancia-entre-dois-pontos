#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(){
	float x1,y1,x2,y2,dist;
	
	printf("\n\n Informe os dois pontos em X e Y respectivamente");
	printf("\n ===============================================\n");
	
	scanf("%f%f%f%f",&x1,&y1,&x2,&y2);
	
	dist = sqrt(pow(x2-x1,2)+pow(y2-y1,2));
	
	printf("A distancia e de: %.2f\n\n", dist);	
}
